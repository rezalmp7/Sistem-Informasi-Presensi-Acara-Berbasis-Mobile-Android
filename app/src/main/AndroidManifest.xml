<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rezalwp.PresensiAcara">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.rezalwp.PresensiAcara.activity.ui.presence.DetailPresenceActivity"></activity>
        <activity
            android:name="com.rezalwp.PresensiAcara.activity.HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rezalwp.PresensiAcara.activity.RegisterActivity" />
        <activity android:name="com.rezalwp.PresensiAcara.activity.DetailActivity" />
        <activity android:name="com.rezalwp.PresensiAcara.activity.LoginAktivity" />
        <activity android:name="com.rezalwp.PresensiAcara.MainActivity"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>